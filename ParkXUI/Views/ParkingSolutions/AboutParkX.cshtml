@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer localizer
@model dynamic

@{
    
    Layout = "_Layout";
}

<div class="section section-breadcrumb">
    <div class="container">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">หน้าแรก</a></li>
            <li class="breadcrumb-item"><a asp-controller="ParkingSolutions" asp-action="AboutParkX" >โซลูชั่นที่จอดรถ</a></li>
            <li class="breadcrumb-item active"><span>ข้อมูลสถานที่จอด</span></li>
        </ol>
    </div><!--container-->
</div>



<div class="section section-banner normal wow fadeIn">∂
    <div class="banner-texture">
        <div class="texture-1 wow fadeInLeftBig" data-wow-duration="1.2s" style="background-image: url(/images/banner-texture--1.svg);"></div>
        <img class="texture-2 wow fadeInLeftBig" data-wow-duration="1.2s" src="images/banner-texture--2.svg" alt="">
    </div>
    <div class="background bottom" style="background-image: url(/images/20200507155917-2020-05-07abountus155916.jpg);">
        <div class="note">ภาพจำลองใช้เพื่องานโฆษณาเท่านั้น</div>
    </div>

    <div class="container">
        <div class="page-title">
            <h2 class="title-xl wow fadeInUp" data-wow-delay="0.35s">ข้อมูลสถานที่จอด</h2>
            <!-- <p class="wow fadeInUp" data-wow-delay="0.45s">พาร์คเอ็กซ์</p> -->
        </div>
    </div><!--container-->
</div><!--section-banner-->

<div class="section section-body pt-0">
    <div class="about-texture" data-bottom-top="transform: translateY(400px) translateZ(0);" data-top-bottom="transform: translateY(-100px) translateZ(0);">
        <img src="img/logo.svg" class="wow fadeInUp" data-wow-delay="0.3s" alt="">
    </div>
    <div class="container">
        <div class="about-wrap">
            <div class="row">
                <div class="col-lg-6">
                    <div class="image wow fadeInUp" data-wow-delay="1s"><img alt="" class="w-100" src="/images/10ec7647.jpg">
                        <div class="img-note left wow fadeIn">*ภาพจำลองใช้เพื่องานโฆษณาเท่านั้น</div>
                    </div>
                </div>

                <div class="col-lg-6">
                    <div class="article wow fadeInUp" data-wow-delay="1.15s">
                         @localizer["CompanyName"]
                         @localizer["ParkDescription"]
                         @localizer["ParkList"]
                        <div class="about-texture d-block d-lg-none"><img alt="" class="wow fadeInUp" data-wow-delay="0.3s" src="images/116a104e.png"></div>
                    </div>
                    <!--article--></div>			
                </div><!--row-->
        </div><!--about-wrap-->

        <div class="about-company">
            <div class="row">
                <div class="col-md-4">
                    <div class="company-quotes wow fadeInUp" data-wow-delay="0.2s">
                        <h3>vision</h3>
                        <p>@localizer["vision"]</p>
                    </div>
                    <!--company-quotes--></div>

                <div class="col-md-4">
                    <div class="company-quotes wow fadeInUp">
                        <h3>Mission</h3>
                        <p>@localizer["mission"]</p>
                    </div>
                    <!--company-quotes--></div>

                <div class="col-md-4">
                    <div class="company-quotes wow fadeInUp" data-wow-delay="0.4s">
                        <h3>Our Value</h3>
                        <p>@localizer["ourvalue"]</p>
                    </div>
                    <!--company-quotes--></div>
            </div>
            <!--row-->		</div>
    </div><!--container-->
</div><!--section-body--> 

